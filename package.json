{
  "name": "Relay-POC",
  "version": "1.0.0",
  "description": "Relay Proof of Concept",
  "main": "index.js",
  "scripts": {
    "installDependencies": "npm install",
    "webpack": "webpack --progress --profile --colors",
    "prebuild": "npm prune",
    "compile": "babel ./src/server -d ./src/dist",
    "build": "npm run installDependencies && npm run webpack && npm run compile && npm run start",
    "build-dev": "npm run installDependencies && npm run webpack-dev && npm run compile && npm run foreman",
    "start": "cross-env NODE_PATH=./src/dist node ./src/dist/index.js",
    "test": "npm run test-node",
    "test-node": "mocha './test/**/*-test.js' --compilers js:babel-register --require ./test/utils/config-dom.js",
    "test-online": "npm run test-node-online",
    "test-node-online": "mocha './test/**/*-test-online.js' --compilers js:babel-register --require ./test/utils/config-dom.js"
  },
  "author": "ProdigyDevTeam",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.9.0",
    "body-parser": "^1.15.1",
    "cookie-parser": "^1.4.3",
    "cross-env": "^1.0.8",
    "express": "^4.13.4",
    "express-graphql": "^0.5.3",
    "graphql": "^0.6.0",
    "isomorphic-fetch": "^2.2.1",
    "path": "^0.12.7",
    "q": "^1.4.1",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.0",
    "redux": "^3.5.1",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-decorators": "^6.8.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-runtime": "6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.2.0",
    "file-loader": "^0.8.5",
    "graphiql": "^0.7.1",
    "ignore-styles": "^2.0.0",
    "jscs": "^3.0.3",
    "jsdom": "^8.4.0",
    "mocha": "^2.4.5",
    "node-sass": "^3.6.0",
    "nodemon": "^1.9.1",
    "react-addons-test-utils": "^15.0.1",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^3.3.1",
    "redux-devtools-chart-monitor": "^1.4.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-filterable-log-monitor": "^0.6.4",
    "redux-devtools-inspector": "^0.5.3",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0"
  }
